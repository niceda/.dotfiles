#!/bin/bash

set file_path "/home/fida/.config/password/skiff"
set passwd [exec cat $file_path]

cd ~/QZ/repo/cmake-build-debug/app/device/
strip -x -w --keep-file-symbols device
gzip device
curl -v --user 'xiongdajun:$passwd' --upload-file "device.gz" https://repo-inf.skiffenergy.com/repository/bin-release/pkg/device-debug.gz
